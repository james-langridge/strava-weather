{
  "buildCommand": "npm run build:all",
  "outputDirectory": "web/dist",
  "functions": {
    "api/index.js": {
      "maxDuration": 10,
      "includeFiles": "api/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}